{
  "name": "@mise/test",
  "version": "1.0.1",
  "description": "testing suite for mise",
  "main": "commis.js",
  "author": "dan b",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^0.52.1",
    "uglify-es": "^3.2.2"
  },
  "files": [
    "commis.js",
    "dist"
  ],
  "scripts": {
    "rollup": "rollup -i commis.js -o dist/commis.js -f umd -n commis -m",
